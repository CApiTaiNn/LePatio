<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vue.controleurCreaFaut">
   <children>
      <Label text="Création d'un fauteuil :">
         <VBox.margin>
            <Insets left="30.0" right="50.0" top="50.0" />
         </VBox.margin>
      </Label>
      <MenuButton fx:id="choixZone" mnemonicParsing="false" text="Choix de la zone">
        <items>
          <MenuItem mnemonicParsing="false" text="Action 1" />
          <MenuItem mnemonicParsing="false" text="Action 2" />
        </items>
         <VBox.margin>
            <Insets bottom="50.0" left="50.0" right="50.0" top="40.0" />
         </VBox.margin>
      </MenuButton>
      <MenuButton fx:id="choixRange" mnemonicParsing="false" text="Choix de la rangée">
        <items>
          <MenuItem mnemonicParsing="false" text="Action 1" />
          <MenuItem mnemonicParsing="false" text="Action 2" />
        </items>
         <VBox.margin>
            <Insets bottom="50.0" left="50.0" />
         </VBox.margin>
      </MenuButton>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Numéro de fauteuils">
               <HBox.margin>
                  <Insets left="50.0" right="20.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="txtFaut">
               <HBox.margin>
                  <Insets right="200.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="ok" mnemonicParsing="false" onMouseClicked="#clicOk" text="Ok">
               <HBox.margin>
                  <Insets right="30.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="annuler" mnemonicParsing="false" onMouseClicked="#clicAnnuler" text="Annuler" />
         </children>
         <opaqueInsets>
            <Insets right="30.0" />
         </opaqueInsets>
      </HBox>
   </children>
</VBox>
